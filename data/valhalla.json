{
  "additional_data": {
    "elevation": "/data/valhalla/elevation/"
  },
  "httpd": {
    "service": {
      "interrupt": "ipc:///tmp/osmscout_server_valhalla_interrupt",
      "listen": "tcp://127.0.0.1:TCPIP_ROUTE_PORT",
      "loopback": "ipc:///tmp/osmscout_server_valhalla_loopback"
    }
  },
  "loki": {
    "actions": [
      "locate",
      "route",
      "height",
      "sources_to_targets",
      "optimized_route",
      "isochrone",
      "trace_route",
      "trace_attributes",
      "transit_available"
    ],
    "logging": {
      "color": false,
      "file_name": "path_to_some_file.log",
      "long_request": 3000.0,
      "type": "osmscout"
    },
    "service": {
      "proxy": "ipc:///tmp/osmscout_server_valhalla_loki"
    },
    "service_defaults": {
      "minimum_reachability": 50,
      "radius": 0
    },
    "use_connectivity": true
  },
  "meili": {
    "auto": {
      "search_radius": 50,
      "turn_penalty_factor": 200
    },
    "bicycle": {
      "turn_penalty_factor": 140
    },
    "customizable": [
      "mode",
      "search_radius",
      "turn_penalty_factor",
      "gps_accuracy",
      "sigma_z",
      "beta",
      "max_route_distance_factor",
      "max_route_time_factor"
    ],
    "default": {
      "beta": 3,
      "breakage_distance": 2000,
      "geometry": false,
      "gps_accuracy": 5.0,
      "interpolation_distance": 10,
      "max_route_distance_factor": 5,
      "max_route_time_factor": 5,
      "max_search_radius": 100,
      "route": true,
      "search_radius": 50,
      "sigma_z": 4.07,
      "turn_penalty_factor": 0
    },
    "grid": {
      "cache_size": 100240,
      "size": 500
    },
    "logging": {
      "color": false,
      "file_name": "path_to_some_file.log",
      "type": "osmscout"
    },
    "mode": "auto",
    "multimodal": {
      "turn_penalty_factor": 70
    },
    "pedestrian": {
      "search_radius": 50,
      "turn_penalty_factor": 100
    },
    "service": {
      "proxy": "ipc:///tmp/osmscout_server_valhalla_meili"
    },
    "verbose": false
  },
  "mjolnir": {
